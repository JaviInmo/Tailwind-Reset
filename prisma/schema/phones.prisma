// Remove the Province and Municipality models from this file
// Only keep the Contact model with references to those models

model Contact {
  id             Int          @id @default(autoincrement())
  name           String
  provinceId     String
  province       Province     @relation(fields: [provinceId], references: [id])
  municipalityId String
  municipality   Municipality @relation(fields: [municipalityId], references: [id])
  personalPhone  String       // Personal phone number
  statePhone     String       // State phone number
  landlinePhone  String       // Landline number
  jobTitle       String       // Job title
  workplace      String       // Workplace
  
  @@map("contacts")
}

// Province and Municipality models are removed from this file
// They are now only defined in incidence.prisma
